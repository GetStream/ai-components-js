{
  "name": "@stream-io/ai-components-react-native",
  "version": "1.0.0",
  "private": true,
  "description": "React Native SDK for AI Components",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/typescript/index.d.ts",
  "exports": {
    ".": {
      "react-native": {
        "import": "./src/index.ts",
        "require": "./dist/commonjs/index.js"
      },
      "default": "./dist/module/index.js",
      "types": "./dist/typescript/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "pnpm run clean && bob build",
    "start": "tsc --project tsconfig.json --watch",
    "eslint": "eslint 'src/**/*.{js,ts,tsx}' --max-warnings 0",
    "lint": "prettier --list-different 'src/**/*.{js,ts,tsx,json}' && yarn run eslint",
    "lint-fix": "prettier --list-different 'src/**/*.{js,ts,tsx,json}' --write && yarn run eslint --fix",
    "clean": "rimraf dist",
    "test": "vitest",
    "test-ci": "echo 'No tests yet'"
  },
  "keywords": [
    "ai",
    "react-native",
    "components",
    "sdk"
  ],
  "author": {
    "name": "GetStream.io, Inc.",
    "url": "https://getstream.io/team/"
  },
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@khanacademy/simple-markdown": "^2.1.0",
    "@stream-io/state-store": "^1.1.6",
    "linkifyjs": "^4.3.2",
    "lodash": "4.17.21",
    "react-syntax-highlighter": "15.5.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@shopify/react-native-skia": "^2.3.10",
    "@types/lodash": "4.17.20",
    "@types/node": "^24",
    "@types/react": "19.2.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "concurrently": "catalog:",
    "expo-image-picker": "^17.0.8",
    "react": "19.2.0",
    "react-native": "^0.82.1",
    "react-native-builder-bob": "0.40.14",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-reanimated": "^4.1.3",
    "rimraf": "^6.0.1",
    "typescript": "catalog:",
    "victory-native": "^41.20.2",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "expo": ">=51.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.73.0",
    "react-native-gesture-handler": ">=2.18.0",
    "react-native-reanimated": ">=3.16.0",
    "react-native-svg": ">=15.8.0",
    "react-syntax-highlighter": ">=15.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "codegenConfig": {
    "name": "AiComponentsReactNative",
    "type": "components",
    "jsSrcsDir": "src/native-specs",
    "android": {
      "javaPackageName": "com.streamio.aicomponents.renderkit"
    }
  },
  "packageManager": "pnpm@10.13.1"
}
